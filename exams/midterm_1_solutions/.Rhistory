knitr::opts_chunk$set(echo = TRUE)
library(knitr)
library(tidyverse)
set.seed(1010)
n <- 20
x1<-rnorm(n,1,1)
x2<-rnorm(n,2,1)
e<-rnorm(n,0,1)
y<-1 + x1+x2+e
sim_data <- data.frame(x1,x2,y)
ggplot(data = sim_data, mapping = aes(x = x1, y = y))+ geom_point(color = "purple") +geom_line()
ggplot(data = sim_data, mapping = aes(x = x1, y = y))+ geom_point(color = "purple") + geom_smooth(se = F)
ggplot(data = sim_data, mapping = aes(x = x1, y = y))+ geom_point(color = "purple") + geom_smooth( method = "lm", color = "red", se = F)
ggplot(data = sim_data, mapping = aes(x = x1, y = y))+ geom_point(color = "purple") + geom_smooth(method = "lm", formula = y ~ poly(x,4), se = F, color = "orange")
ggplot(data = sim_data, mapping = aes(x = x1, y = y))+ geom_point(color = "purple") + geom_smooth(method = "lm", formula = y ~ poly(x,10), se = F, color = "yellow")
dip <- lm(y~x1, data= sim_data)
pope <- summary(dip)
mean(pope$residuals^2)
moore <- read_delim("data/moore.csv", ";",
escape_double = FALSE, trim_ws = TRUE)
ggplot(data = moore, mapping = aes(x = Transistors, y = Date))+ geom_point(color = "purple")
ggplot(data = moore, mapping = aes(x = Transistors, y = Date))+ geom_point(color = "purple") + geom_smooth(color = "red", se = F)
shop <- lm(Date ~ Transistors, data = moore)
poop <- data.frame(Date = c(1970, 2000, 2020))
predict(shop, newdata=poop)
plot(shop)
ggplot(data = moore, mapping = aes(x = log2(Transistors), y = Date))+ geom_point() + geom_smooth(method = "lm", se = F)
dink <- lm(Date ~ log2(Transistors), data = moore)
summary(dink)
plot(dink)
quantile(dink, 0.025)
