names_to = "metric",
values_to = "value" )
selection_metrics %>%
ggplot(   aes(x = n_var, y = value, color = metric))+geom_line()
selection_metrics %>% filter(metric=="fs_bic") %>% arrange(value) %>% head()
selection_metrics %>% filter(metric=="bs_bic") %>% arrange(value) %>% head()
