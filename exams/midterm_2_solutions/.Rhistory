bs_mod<-regsubsets(fat ~., data = ir_train, nvmax = 100, method = "backward")
selection_metrics<- bind_cols(
fs_bic=summary(fs_mod)$bic,
bs_bic=summary(bs_mod)$bic,
n_var=1:100
)
